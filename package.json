    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "body-parser": "^1.19.0",
    "electron-is-dev": "^1.1.0",
    "electron-reload": "^1.5.0",
    "express": "^4.17.1",
    "node-onvif": "^0.1.7",
    "onvif": "^0.6.2",
    "react": "^16.13.0",
